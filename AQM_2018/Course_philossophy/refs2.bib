@Article{Waltari2007,
  Title                    = {Locating pleistocene refugia: comparing phylogeographic and ecological niche model predictions},
  Author                   = {Waltari, Eric and Hijmans, Robert J. and Peterson, A. Townsend and Nyari, Arpad S. and Perkins, Susan L. and Guralnick, Robert P.},
