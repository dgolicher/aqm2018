@Article{Guisan2007a,
  Title                    = {What matters for predicting the occurrences of trees: Techniques, data, or species{'} characteristics?},
  Author                   = {Guisan, A. and Zimmermann, N. E. and Elith, J. and Graham, C. H. and Phillips, S. and Peterson, A. T.},
