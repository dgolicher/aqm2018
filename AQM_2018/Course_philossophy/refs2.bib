@Article{Guisan2006a,
  Title                    = {Using Niche-Based Models to Improve the Sampling of Rare Species},
  Author                   = {Guisan, Antoine and Broennimann, Olivier and Engler, Robin and Vust, Mathias and Yoccoz, Nigel G. and Lehmann, Anthony and Zimmermann, Niklaus E.},
  Journal                  = {Conservation Biology},
